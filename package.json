{
  "name": "v2ray-sub-worker",
  "version": "1.0.0",
  "description": "V2Ray Subscription Proxy & Toolkit - Deploy on Cloudflare Workers/Pages. Proxy, convert, merge, and manage V2Ray subscriptions.",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build --mode client && vite build",
    "preview": "npm run build && wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "v2ray",
    "subscription",
    "proxy",
    "cloudflare-workers",
    "cloudflare-pages",
    "hono",
    "react",
    "vless",
    "vmess",
    "trojan",
    "clash",
    "singbox"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/vite-build": "^1.0.0",
    "hono": "^4.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "wrangler": "^3.0.0"
  }
}
